{
  "name": "scripted-browser",
  "displayName": "scripted-browser",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Custom Commands",
      "properties": {
        "scripted-browser.scripts": {
          "type": "array",
          "default": [],
          "description": "Define custom commands to run scripts and open URLs."
        }
      }
    },
    "commands": [
      {
        "command": "scripted-browser.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "scripted-browser.scripted-tab",
        "title": "Scripted Tab"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0"
  }
}